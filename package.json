{
  "name": "parcel-mermaid-import-issue",
  "version": "0.0.1",
  "description": "Minimal code sample that reproduces an import issue between parcel/mermaid",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "webpack -w --config webpack.config.js",
    "build": "webpack --config webpack.config.js",
    "start:parcel": "parcel watch src/manifest.json --host localhost --config @parcel/config-webextension",
    "build:parcel": "parcel build --no-optimize src/manifest.json --config @parcel/config-webextension",
    "start-sample-page": "parcel serve ./sample-page/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwmkerr/parcel-mermaid-import-issue.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dwmkerr/parcel-mermaid-import-issue/issues"
  },
  "homepage": "https://github.com/dwmkerr/parcel-mermaid-import-issue#readme",
  "dependencies": {
    "mermaid": "^10.0.2"
  },
  "devDependencies": {
    "@parcel/config-webextension": "^2.8.3",
    "copy-webpack-plugin": "^11.0.0",
    "parcel": "^2.8.3",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.3",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1"
  }
}
